{
  "name": "appium-test-support",
  "description": "A collection of test utilities used across Appium packages",
  "keywords": [
    "appium"
  ],
  "version": "1.0.0",
  "author": "appium",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/appium/appium-test-support.git"
  },
  "bugs": {
    "url": "https://github.com/appium/appium-test-support/issues"
  },
  "engines": [
    "node"
  ],
  "main": "./build/index.js",
  "bin": {
    "android-emu-travis-pre": "./bin/android-emu-travis-pre.sh",
    "android-emu-travis-post": "./bin/android-emu-travis-post.sh"
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "appium-support": "^2.5.0",
    "babel-runtime": "=5.8.24",
    "bluebird": "^3.5.1",
    "lodash": "^4.17.5",
    "sinon": "^6.0.0"
  },
  "scripts": {
    "prepublish": "gulp prepublish",
    "test": "gulp once",
    "watch": "gulp watch",
    "e2e-test": "gulp transpile && mocha build/test/e2e/ -t 10000000",
    "build": "gulp transpile",
    "coverage": "gulp coveralls",
    "lint": "gulp eslint",
    "mocha": "mocha",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "pre-commit": [
    "precommit-msg",
    "test"
  ],
  "devDependencies": {
    "appium-gulp-plugins": "^2.2.2",
    "babel-eslint": "^7.1.1",
    "chai": "^4.1.2",
    "colors": "^1.1.2",
    "eslint": "^3.10.2",
    "eslint-config-appium": "^3.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-promise": "^3.3.1",
    "gulp": "^3.8.11",
    "mocha": "^5.0.5",
    "pre-commit": "^1.2.2"
  },
  "greenkeeper": {
    "ignore": [
      "babel-eslint",
      "babel-preset-env",
      "eslint",
      "eslint-plugin-babel",
      "eslint-plugin-import",
      "eslint-plugin-mocha",
      "eslint-plugin-promise",
      "gulp",
      "babel-runtime"
    ]
  }
}
